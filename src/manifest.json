{
  "package": "com.example.fastly",
  "name": "fastly",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1070,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "pages/Toast",
    "pages": {
      "pages/Button": {
        "component": "index"
      },
      "pages/Tag": {
        "component": "index"
      },
      "pages/Toast": {
        "component": "index"
      },
      "pages/Input": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "pages": {
      "pages/Button": {
        "titleBarText": "FastlyUi组件库"
      },
      "pages/Tag": {
        "titleBarText": "FastlyUi组件库"
      },
      "pages/Toast": {
        "titleBarText": "FastlyUi组件库"
      },
      "pages/Input": {
        "titleBarText": "FastlyUi组件库"
      }
    }
  }
}
